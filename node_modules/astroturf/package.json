{
  "name": "astroturf",
  "version": "1.0.1",
  "description": "Extract and process inline CSS literals in JavaScript files",
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "types": "esm/index.d.ts",
  "publishConfig": {
    "directory": "lib"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/4Catalyzer/astroturf.git"
  },
  "bin": {
    "astroturf": "./cli.js"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "!(__file_snapshots__/)*.js": [
      "eslint --fix",
      "yarn 4c format"
    ],
    "README.md": [
      "doctoc"
    ]
  },
  "prettier": {
    "printWidth": 79,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "jest": {
    "roots": [
      "<rootDir>/test"
    ],
    "setupFilesAfterEnv": [
      "./test/setup.js"
    ],
    "watchPathIgnorePatterns": [
      "build",
      "output",
      "__file_snapshots__"
    ]
  },
  "author": "4Catalyzer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/4Catalyzer/astroturf/issues"
  },
  "homepage": "https://github.com/4Catalyzer/astroturf#readme",
  "dependencies": {
    "@babel/code-frame": "^7.16.0",
    "@babel/core": "^7.16.5",
    "@babel/generator": "^7.16.5",
    "@babel/helper-module-imports": "^7.16.0",
    "@babel/plugin-transform-react-jsx": "^7.16.5",
    "@babel/template": "^7.16.0",
    "@babel/traverse": "^7.16.5",
    "@babel/types": "^7.16.0",
    "@modular-css/processor": "^27.0.3",
    "common-tags": "^1.8.2",
    "cosmiconfig": "^7.0.1",
    "css-loader": "^5.1.3",
    "fast-levenshtein": "^3.0.0",
    "find-cache-dir": "^3.3.2",
    "globby": "^11.0.0",
    "json5": "^2.2.0",
    "lodash": "^4.17.21",
    "magic-string": "^0.25.7",
    "picocolors": "^1.0.0",
    "postcss": "^8.4.5",
    "postcss-nested": "^5.0.6",
    "postcss-scss": "^4.0.2",
    "resolve": "^1.20.0",
    "webpack-virtual-modules": "^0.3.2",
    "yargs": "^17.3.0"
  },
  "peerDependencies": {
    "webpack": ">=5"
  }
}
