{
  "name": "project",
  "version": "1.0.0",
  "description": "project with webpack",
  "main": "index.js",
  "private": true,
  "scripts": {
    "build:sass": "sass src/styles/main.scss src/styles/index.css",
    "dev": "cross-env NODE_ENV=development webpack --mode development",
    "build": "npm run build:sass && cross-env NODE_ENV=production webpack --mode production",
    "watch": "cross-env NODE_ENV=development webpack --mode development --watch",
    "start": "webpack-dev-server --mode development --open",
    "stats": "webpack --json > stats.json && webpack-bundle-analyzer stats.json"
  },
  "keywords": [
    "js",
    "javascript",
    "webpack"
  ],
  "author": "Rostran Fabio <rostranespinoza@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/preset-env": "^7.16.11",
    "astroturf": "^1.0.1",
    "babel-loader": "^8.2.4",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^10.2.4",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^3.4.1",
    "html-webpack-plugin": "^5.5.0",
    "less": "^4.1.2",
    "less-loader": "^10.2.0",
    "mini-css-extract-plugin": "^2.6.0",
    "node-sass": "^7.0.1",
    "papaparse": "^5.3.2",
    "postcss": "^8.4.12",
    "postcss-loader": "^6.2.1",
    "postcss-preset-env": "^7.4.3",
    "pug": "^2.0.4",
    "pug-loader": "^2.4.0",
    "sass": "^1.50.0",
    "sass-loader": "^12.6.0",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.1",
    "webpack": "^5.65.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.8.1"
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "expose-loader": "^3.1.0",
    "jquery": "^3.6.0",
    "jss": "^10.9.0",
    "jss-plugin-global": "^10.9.0",
    "jss-plugin-vendor-prefixer": "^10.9.0",
    "jss-preset-default": "^10.9.0",
    "normalize.css": "^8.0.1"
  }
